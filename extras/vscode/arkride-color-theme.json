  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6b7888",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#52B5A4"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#F8BA65"
      }
    },
    {
      "name": "Boolean",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#F8BA65"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#F8BA65"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "keyword.control", "keyword.operator.new"],
      "settings": {
        "foreground": "#EA7475"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#8a96a4"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#EA7475"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#5080C0"
      }
    },
    {
      "name": "Method",
      "scope": ["meta.method-call", "entity.name.method"],
      "settings": {
        "foreground": "#5080C0"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": {
        "foreground": "#A4D8EC"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type", "storage.type.primitive"],
      "settings": {
        "foreground": "#A4D8EC"
      }
    },
    {
      "name": "Interface",
      "scope": ["entity.name.type.interface"],
      "settings": {
        "foreground": "#A4D8EC"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#cdd4dc"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "variable.other.object.property", "support.variable.property"],
      "settings": {
        "foreground": "#7F7B78"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#EA7475"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#F8BA65"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#8a96a4"
      }
    },
    {
      "name": "Import",
      "scope": ["keyword.control.import", "keyword.control.export", "keyword.control.from"],
      "settings": {
        "foreground": "#A4D8EC"
      }
    },
    {
      "name": "Decorator",
      "scope": ["meta.decorator", "punctuation.decorator"],
      "settings": {
        "foreground": "#E07840"
      }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#A4D8EC"
      }
    },
    {
      "name": "Escape",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#A4D8EC"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#D94848"
      }
    },
    {
      "name": "Deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#A86078",
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#5080C0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#EA7475",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#F8BA65",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#A4D8EC"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code.block"],
      "settings": {
        "foreground": "#52B5A4"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#5080C0"
      }
    },
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#A4D8EC"
      }
    },
    {
      "name": "CSS Selector",
      "scope": ["entity.name.tag.css", "entity.other.attribute-name.class.css", "entity.other.attribute-name.id.css"],
      "settings": {
        "foreground": "#EA7475"
      }
    }
  ]
}
